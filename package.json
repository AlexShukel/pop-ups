{
    "name": "reactive-popups",
    "version": "0.0.1",
    "description": "Easy popup management with React",
    "main": "./dist/index.js",
    "module": "./dist/reactive-popups.esm.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "aqu build",
        "start": "aqu watch",
        "lint": "aqu lint",
        "lint:fix": "aqu lint --fix",
        "test": "aqu test --passWithNoTests",
        "test:watch": "aqu test --passWithNoTests --watch",
        "test:coverage": "aqu test --silent --ci --coverage --coverageReporters=\"text\" --coverageReporters=\"text-summary\"",
        "prepublishOnly": "aqu build",
        "release": "np",
        "size": "size-limit",
        "size:why": "size-limit --why"
    },
    "author": "Aleksandras Šukelovič",
    "license": "MIT",
    "homepage": "https://github.com/AlexShukel/reactive-popups#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AlexShukel/reactive-popups.git"
    },
    "bugs": {
        "url": "https://github.com/AlexShukel/reactive-popups/issues"
    },
    "devDependencies": {
        "@types/react": "^17.0.11",
        "@types/react-dom": "^17.0.8",
        "aqu": "0.1.0",
        "np": "^7.5.0",
        "react-dom": "^17.0.2",
        "typescript": "^4.3.4"
    },
    "types": "./dist/reactive-popups.d.ts",
    "peerDependencies": {
        "react": ">=16"
    },
    "dependencies": {
        "tiny-invariant": "^1.1.0"
    }
}

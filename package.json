{
    "name": "reactive-popups",
    "version": "0.5.4",
    "description": "Easy popup management with React",
    "main": "./dist/index.js",
    "types": "./.tsup/declaration/index.d.ts",
    "module": "./dist/index.mjs",
    "files": [
        "dist",
        ".tsup"
    ],
    "publishConfig": {
        "directory": "prepared-package"
    },
    "scripts": {
